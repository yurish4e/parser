extends layout

block content

  div(class=['row'])
    div(class=['col', 's10', 'offset-s1', 'card'])
      h2 Result:
      table(class=['striped']) 
        thead
          tr
            th Project Name
            th Status
            th Pages
            th Date
        tbody
          
            - each item in resultPage
              tr
                td= item.name
                td= item.status
                td= item.pages
                td= item.date
